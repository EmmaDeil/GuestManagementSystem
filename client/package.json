{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev",
    "build": "cross-env NODE_ENV=production next build",
    "build:linux": "NODE_ENV=production next build",
    "build:debug": "cross-env NODE_ENV=production NEXT_DEBUG=1 next build",
    "start": "cross-env NODE_ENV=production next start",
    "start:linux": "NODE_ENV=production next start",
    "start:dev": "cross-env NODE_ENV=development next start",
    "render-build": "npm install && cross-env NODE_ENV=production next build",
    "render-start": "cross-env NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rmdir /s /q .next 2>nul || echo Clean completed",
    "debug:env": "node -e \"console.log('NODE_ENV:', process.env.NODE_ENV); console.log('NEXT_PUBLIC_API_URL:', process.env.NEXT_PUBLIC_API_URL); console.log('NEXT_PUBLIC_CLIENT_URL:', process.env.NEXT_PUBLIC_CLIENT_URL);\""
  },
  "dependencies": {
    "@types/node": "^20",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.2",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "next": "15.5.4",
    "postcss": "^8.5.6",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "tailwindcss": "^3.4.18",
    "typescript": "^5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3"
  }
}
